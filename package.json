{
  "name": "rapid.js",
  "version": "1.2.3",
  "description": "",
  "main": "dist/rapid.js",
  "scripts": {
    "test": "npm run unit && npm run lint",
    "compile": "babel --presets=es2015 --no-comments -d dist src",
    "copy-dev": "npm run compile && rm -rf ./demo/rapid && cp -R ./dist ./demo/rapid",
    "prepare": "npm run compile",
    "lint": "eslint --ext .js src test",
    "lint:fix": "npm run lint -- --fix",
    "unit": "jest --maxWorkers 2",
    "unit:watch": "jest test/**/*.test.js --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rapidjs/rapid.js.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/rapidjs/rapid.js/issues"
  },
  "homepage": "https://github.com/rapidjs/rapid.js#readme",
  "devDependencies": {
    "babel-cli": "^6.24.0",
    "babel-eslint": "^8.0.2",
    "babel-preset-es2015": "^6.24.0",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-config-airbnb-standard": "^1.6.5",
    "eslint-plugin-import": "^2.8.0",
    "jest": "^21.2.1"
  },
  "dependencies": {
    "lodash": "^4.17.4",
    "pluralize": "^4.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "jest": {
    "collectCoverage": true
  }
